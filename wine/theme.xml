<theme>
    <formatVersion>3</formatVersion>
    <include>./../common.xml</include>
	<include>./../common/templates/landscape.xml</include>

	<view name="system">
	<!-- System Specific Images -->
		<image name="logo">
			<path>./logo.svg</path>			
        </image>		
	</view>

	<view name="basic, detailed">
	<!-- System Specific Images -->
		<image name="logo">
			<path>./logo.svg</path>
<!-- Logo Alignment Specific to bgconsole.png -->
			<origin>0 0.63</origin>
			<pos>0.02 0.1000</pos>
			<maxSize>0.55 0.12</maxSize>
		</image>
				
		<image name="background" extra="true">
			<path>./bgconsole.png</path>
			<origin>0 0</origin>
			<pos>0 0</pos>
			<size>1 1</size>
			<tile>false</tile>
</image>
	</view>
</theme>